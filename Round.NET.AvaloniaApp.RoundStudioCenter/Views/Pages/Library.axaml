<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="Round.NET.AvaloniaApp.RoundStudioCenter.Views.Pages.Library">
    <TabControl Margin="5">
        <TabItem Margin="5" Header="全部">
            <ScrollViewer>
                <StackPanel>
                    <Expander Header="从外部安装" Name="InstalledOutside" IsExpanded="True" Padding="5" Margin="0,5,0,5"></Expander>
                    <Expander Header="已安装" Name="InstalledInside" IsExpanded="True" Padding="5" Margin="0,5,0,5"></Expander>
                    <Expander Header="已拥有" Name="Owned" IsVisible="False" Padding="5" Margin="0,5,0,5"></Expander>
                </StackPanel>
            </ScrollViewer>
        </TabItem>
        <TabItem Margin="5" Header="更新">
            <ScrollViewer>
                <StackPanel>
                    <Expander Header="已安装" Name="InstalledInsideUpdate" IsExpanded="True" Padding="5" Margin="0,5,0,5"></Expander>
                </StackPanel>
            </ScrollViewer>
        </TabItem>
    </TabControl>
</UserControl>
