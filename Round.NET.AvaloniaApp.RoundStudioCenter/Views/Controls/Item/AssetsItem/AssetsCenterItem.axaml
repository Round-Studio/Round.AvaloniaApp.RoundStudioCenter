<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:fluentIcons="clr-namespace:FluentAvalonia.FluentIcons;assembly=FluentAvalonia.FluentIcons"
             xmlns:item1="clr-namespace:Round.NET.AvaloniaApp.RoundStudioCenter.Views.Controls.Item"
             xmlns:button1="clr-namespace:Round.NET.AvaloniaApp.RoundStudioCenter.Views.Controls.Button"
             mc:Ignorable="d" d:DesignWidth="800"
             x:Class="Round.NET.AvaloniaApp.RoundStudioCenter.Views.Controls.Item.AssetsItem.AssetsCenterItem">
    <item1:ItemBox>
        <item1:ItemBox.Content>
            <DockPanel LastChildFill="True">
                <DockPanel DockPanel.Dock="Right" HorizontalAlignment="Right" Margin="10">
                    <button1:AccentIconButton Icon="Link20Regular" Click="Button_OnClick">详情页</button1:AccentIconButton>
                </DockPanel>
                <Border Margin="8" CornerRadius="6" DockPanel.Dock="Left" Width="56" Height="56" HorizontalAlignment="Left" Name="AssetsIcon">
                    <fluentIcons:FluentIcon Icon="Grid20Regular" Width="40" Height="40"/>
                </Border>
                <StackPanel Margin="8" HorizontalAlignment="Left" VerticalAlignment="Center">
                    <TextBlock FontSize="16" FontWeight="Bold" Name="AssetsName">Name</TextBlock>
                    <TextBlock Name="AssetsProfile">AssetsProfile</TextBlock>
                </StackPanel>
            </DockPanel>
        </item1:ItemBox.Content>
    </item1:ItemBox>
</UserControl>
