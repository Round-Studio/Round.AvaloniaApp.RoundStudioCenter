<ui:Frame xmlns="https://github.com/avaloniaui"
          xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
          xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
          xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
          xmlns:ui="clr-namespace:FluentAvalonia.UI.Controls;assembly=FluentAvalonia"
          mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
          x:Class="Round.NET.AvaloniaApp.RoundStudioCenter.Views.Controls.Frame">
    <ui:Frame.Transitions>
        <Transitions>
            <DoubleTransition Duration="0:0:0.3" Easing="ExponentialEaseOut" Property="Opacity"></DoubleTransition>
            <ThicknessTransition Property="Margin" Easing="ExponentialEaseOut" Duration="0:0:0.3"/>
        </Transitions>
    </ui:Frame.Transitions>
</ui:Frame>